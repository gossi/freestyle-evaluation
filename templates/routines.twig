<table class="table table-striped table-condensed table-bordered">
<thead>
	<tr>
		<th rowspan="2">Name</th>
		<th colspan="{{ 6 + group.getPerformanceJudges().size() }}">Performance: Total</th>
		<th rowspan="2" style="width: 10px"></th>
		<th colspan="{{ 6 + group.getPerformanceJudges().size() }}">Performance: Execution</th>
		<th rowspan="2" style="width: 10px"></th>
		<th colspan="{{ 6 + group.getPerformanceJudges().size() }}">Performance: Choreography</th>
		<th rowspan="2" style="width: 10px"></th>
		<th colspan="{{ 6 + group.getPerformanceJudges().size() }}">Performance: Music and Timing</th>
	</tr>
	<tr>
		<th>AVG</th>
		<th>MIN</th>
		<th>MAX</th>
		<th>RANGE</th>
		<th>SD</th>
		<th>V</th>
		{% for p in group.getPerformanceJudges() %}
			<th>{{ p.getName() }}</th>
		{% endfor %}
		
		<th>AVG</th>
		<th>MIN</th>
		<th>MAX</th>
		<th>RANGE</th>
		<th>SD</th>
		<th>V</th>
		{% for p in group.getPerformanceJudges() %}
			<th>{{ p.getName() }}</th>
		{% endfor %}
		
		<th>AVG</th>
		<th>MIN</th>
		<th>MAX</th>
		<th>RANGE</th>
		<th>SD</th>
		<th>V</th>
		{% for p in group.getPerformanceJudges() %}
			<th>{{ p.getName() }}</th>
		{% endfor %}
		
		<th>AVG</th>
		<th>MIN</th>
		<th>MAX</th>
		<th>RANGE</th>
		<th>SD</th>
		<th>V</th>
		{% for p in group.getPerformanceJudges() %}
			<th>{{ p.getName() }}</th>
		{% endfor %}
	</tr>
</thead>
<tbody>
{% for routine in group.getRoutines() %}
<tr>
	<td style="white-space: nowrap;">{{ routine.getName() }}</td>
	
	{% set total = analyzer.analyze(routine) %}
	<td class="right-align">{{ "%.2f"|format(total.getAvg()) }}</td>
	<td class="right-align">{{ "%.2f"|format(total.getMin()) }}</td>
	<td class="right-align">{{ "%.2f"|format(total.getMax()) }}</td>
	<td class="right-align">{{ "%.2f"|format(total.getRange()) }}</td>
	<td class="right-align">{{ "%.2f"|format(total.getSd()) }}</td>
	<td class="right-align">{{ "%.2f"|format(total.getVariance()) }}</td>
	
	{% for i in 1..group.getPerformanceJudges().size() %}
		<td class="right-align">{{ "%.2f"|format(routine.getPerformanceScore(i).getTotal()) }}</td>
	{% endfor %}
	<td></td>
	
	{% set execution = analyzer.analyze(routine, 'execution') %}
	<td class="right-align">{{ "%.2f"|format(execution.getAvg()) }}</td>
	<td class="right-align">{{ "%.2f"|format(execution.getMin()) }}</td>
	<td class="right-align">{{ "%.2f"|format(execution.getMax()) }}</td>
	<td class="right-align">{{ "%.2f"|format(execution.getRange()) }}</td>
	<td class="right-align">{{ "%.2f"|format(execution.getSd()) }}</td>
	<td class="right-align">{{ "%.2f"|format(execution.getVariance()) }}</td>
	
	{% for i in 1..group.getPerformanceJudges().size() %}
		<td class="right-align">{{ "%.2f"|format(routine.getPerformanceScore(i).getExecution()) }}</td>
	{% endfor %}
	<td></td>
	
	{% set choreography = analyzer.analyze(routine, 'choreography') %}
	<td class="right-align">{{ "%.2f"|format(choreography.getAvg()) }}</td>
	<td class="right-align">{{ "%.2f"|format(choreography.getMin()) }}</td>
	<td class="right-align">{{ "%.2f"|format(choreography.getMax()) }}</td>
	<td class="right-align">{{ "%.2f"|format(choreography.getRange()) }}</td>
	<td class="right-align">{{ "%.2f"|format(choreography.getSd()) }}</td>
	<td class="right-align">{{ "%.2f"|format(choreography.getVariance()) }}</td>
	
	{% for i in 1..group.getPerformanceJudges().size() %}
		<td class="right-align">{{ "%.2f"|format(routine.getPerformanceScore(i).getChoreography()) }}</td>
	{% endfor %}
	<td></td>
	
	{% set music = analyzer.analyze(routine, 'music') %}
	<td class="right-align">{{ "%.2f"|format(music.getAvg()) }}</td>
	<td class="right-align">{{ "%.2f"|format(music.getMin()) }}</td>
	<td class="right-align">{{ "%.2f"|format(music.getMax()) }}</td>
	<td class="right-align">{{ "%.2f"|format(music.getRange()) }}</td>
	<td class="right-align">{{ "%.2f"|format(music.getSd()) }}</td>
	<td class="right-align">{{ "%.2f"|format(music.getVariance()) }}</td>
	
	{% for i in 1..group.getPerformanceJudges().size() %}
		<td class="right-align">{{ "%.2f"|format(routine.getPerformanceScore(i).getMusicAndTiming()) }}</td>
	{% endfor %}
</tr>
{% endfor %}
</tbody>
</table>

